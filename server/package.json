{
  "name": "server",
  "version": "1.2.0",
  "description": "",
  "author": "Artur Chumak",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "USE_SOCKET=true node -r ts-node/register .",
    "dev": "USE_SOCKET=true nodemon -r ts-node/register .",
    "windev": "nodemon -r ts-node/register .",
    "debug": "USE_SOCKET=true  nodemon  -r ts-node/register --inspect .",
    "windebug": "nodemon  -r ts-node/register --inspect .",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext js --ext ts",
    "ci": "npm run lint && npm run test",
    "db:inspect": "rm -rf models/; node scripts/db-inspect; eslint models/ --no-ignore --fix --ext ts",
    "createUser": "node -r ts-node/register scripts/create-start-user.ts",
    "synchronizeUsers": "node -r ts-node/register scripts/synchronize_users.ts",
    "migrateUsers": "node -r ts-node/register scripts/migrate_users.ts"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@sentry/node": "^5.0.6",
